{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "~/.arduino15/packages/esp8266/tools/**",
                "~/.arduino15/packages/esp8266/hardware/esp8266/2.5.2/**",
                "~/Arduino/libraries/Adafruit_SSD1306/**",
                "~/Arduino/libraries/Adafruit_GFX_Library/*",
                "~/Arduino/libraries/Adafruit_Unified_Sensor/*",
                "~/Arduino/libraries/DHT_sensor_library/*",
                "~/Arduino/libraries/Adafruit_seesaw_Library/*",
                "~/Arduino/libraries/ArduinoJson/*",
                "~/Arduino/libraries/Painless_Mesh/*",
                "~/Arduino/libraries/TaskScheduler/*",
                "~/Arduino/libraries/ESPAsyncTCP/*",
                "~/.platformio/lib/**",
                "${workspaceFolder}/**",
                "${workspaceFolder}/esp8266_src/**",
                "${workspaceFolder}/esp8266_src/lib/**"
            ],
            "defines": [
                "_ESP8266"
            ],
            "compilerPath": "/usr/bin/g++",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": ""
            },
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64",
            "forcedInclude": []
        }
    ],
    "version": 4
}